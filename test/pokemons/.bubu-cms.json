{
  "serve": 3000,
  "storage": "memory",
  "types": [{
    "name": "pokemon",
    "defaultIcon": "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/official-artwork/${pkid}.png",
    "fields": [{
      "name": "pkid",
      "type": "number"
    }, {
      "name": "speciesId",
      "type": "number"
    }, {
      "name": "height",
      "type": "number"
    }, {
      "name": "weight",
      "type": "number"
    }]
  }, {
    "name": "move",
    "defaultIcon": "sap-icon://journey-depart",
    "fields": [{
      "name": "mvid",
      "type": "number"
    }, {
      "name": "power",
      "type": "number"
    }, {
      "name": "pp",
      "type": "number"
    }, {
      "name": "accuracy",
      "type": "number"
    }]
  }, {
    "name": "pokemon_move",
    "fields": [{
      "name": "level",
      "type": "number"
    }]
  }],
  "loaders": [{
    "custom": "./pokemon.js"
  }]
}