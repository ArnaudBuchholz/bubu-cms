{
	"sap.app": {
		"id": "bubu-cms",
		"type": "application",
		"dataSources": {
			"mainService": {
				"uri": "/api/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"libs": {
				"sap.ui.core": {
					"lazy": false
				},
				"sap.m": {
					"lazy": false
				}
			}
		},
		"rootView": {
			"viewName": "bubu-cms.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "bubu-cms.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "InlineRepeat"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}
