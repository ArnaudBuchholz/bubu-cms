  "bugs": {
    "url": "https://github.com/ArnaudBuchholz/bubu-cms/issues"
  },
  "homepage": "https://github.com/ArnaudBuchholz/bubu-cms/",
  "dependencies": {
    "colors": "^1.4.0",
    "mime": "^2.5.2",
    "reserve": "^1.11.4",
    "reserve-odata": "^0.8.0"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "standard": "^16.0.3",
    "ts-standard": "^10.0.0",
    "tsc": "^2.0.3"
