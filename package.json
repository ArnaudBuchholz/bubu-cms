{
  "name": "bubu-cms",
  "version": "0.9.1",
  "private": true,
  "description": "Small, simple & flat CMS for bubu's family",
  "directories": {},
  "scripts": {
    "tsc": "tsc",
    "start": "node .",
    "debug": "node --inspect-brk .",
    "lint": "ts-standard --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnaudBuchholz/bubu-cms.git"
  },
  "author": "Arnaud Buchholz <arnaud.buchholz@free.fr> (http://gpf-js.blogspot.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnaudBuchholz/bubu-cms/issues"
  },
  "homepage": "https://github.com/ArnaudBuchholz/bubu-cms/",
  "dependencies": {
    "colors": "^1.4.0",
    "mime": "^2.5.2",
    "reserve": "^1.11.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-standard": "^10.0.0",
    "tsc": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "ts-standard": {
    "env": [
      "node",
      "jest"
    ],
    "project": "./tsconfig.json",
    "ignore": [
      "/dist/"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "\\.js$"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
